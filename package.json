  "scripts": {
    "postinstall": "npm run build:prod",
    "heroku-postbuild": "npm run build",
    "build": "npm run build:client && npm run build:prod",
    "build:client": "webpack --mode=production",
    "build:prod": "npm run clean:dist && babel ./server --out-dir dist --copy-files",
    "clean:dist": "rimraf ./dist",
    "start:dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --watch --open --hot",
    "start": "node dist/index.js",
    "test:jest": "jest client/tests",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/client/tests/__mocks__/fileMock.js",
